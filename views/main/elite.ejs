                            required: true
                        },
                        code: {
                            required: true
                        } ,
                        mobile: {
                            required: true
                        }
                    },
                    submitHandler: function(form) {
                        $('#onewayForm').find('.btn-search').text(`Loading...`)
                        $('#onewayForm').find('.btn-search').prop('disabled', true)
                        form.submit();
                    }
                })

                $('#roundForm').validate({
                    rules: {
                        from: {
                            required: true,
                        },
                        to: {
                            required: true
                        },
                        departdate: {
                            required: true
                        },
