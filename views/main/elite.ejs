                        $('#roundForm').find('.btn-search').text(`Loading...`)
                        $('#roundForm').find('.btn-search').prop('disabled', true)
                        form.submit();
                    }
                })
                $('#multicityForm').validate({
                    rules: {
                        from: {
                            required: true,
                        },
                        to: {
                            required: true
                        },
                        departdate: {
                            required: true
                        },
                        class: {
                            required: true
                        },
