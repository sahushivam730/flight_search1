        $(".increase-btn").on("click", function (e) {
            e.preventDefault();
            var inputField = $(this).prev("input");
            var currentInputValue = parseInt(inputField.val());
            var cInpVal = parseInt($(this).parents('.col-lg-3').find('.adult').val()) + parseInt($(this).parents('.col-lg-3').find('.child').val()) + parseInt($(this).parents('.col-lg-3').find('.infants').val());
            if(currentInputValue!=9 && cInpVal != 9){
                inputField.val(currentInputValue + 1);
                var totTV = parseInt($(this).parents('.col-lg-3').find('.adult').val()) + parseInt($(this).parents('.col-lg-3').find('.child').val()) + parseInt($(this).parents('.col-lg-3').find('.infants').val());
                if(totTV==1){
