                    $(this).parents('.col-lg-3').find('.nooftarvellers').text(totTV+" Adult")
                }else{
                    $(this).parents('.col-lg-3').find('.noofpassenger').text(totTV+" passengers")
                    $(this).parents('.col-lg-3').find('.nooftarvellers').text(totTV+" Travellers")
                }
            }
        });

        $(".decrease-btn").on("click", function (e) {
            e.preventDefault();
            var inputField = $(this).next("input");
            var currentInputValue = parseInt(inputField.val());
            // alert(currentInputValue);
            if(inputField.hasClass('child')==true){
                if(currentInputValue>0){
                    inputField.val(currentInputValue - 1);
                    var totTV = parseInt($('.adult').val()) + parseInt($('.child').val()) + parseInt($('.infants').val())
                    if(totTV==1){
                        $('.noofpassenger').text(totTV+" passenger")
                        $('.nooftarvellers').text(totTV+" Adult")
                    }else{
                        $('.noofpassenger').text(totTV+" passengers")
                        $('.nooftarvellers').text(totTV+" Travellers")
                    }
                }
            }else{
                if(currentInputValue > 0){
                    inputField.val(currentInputValue - 1);
                    var totTV = parseInt($('.adult').val()) + parseInt($('.child').val()) + parseInt($('.infants').val())
                    if(totTV==1){
                        $('.noofpassenger').text(totTV+" passenger")
                        $('.nooftarvellers').text(totTV+" Adult")
                    }else{
                        $('.noofpassenger').text(totTV+" passengers")
                        $('.nooftarvellers').text(totTV+" Travellers")
                    }
                }
            }
            // if (currentInputValue < 1) {
            //     inputField.val("0");
            // }
        });

        $('input[name="class"]').on('click',function () {
